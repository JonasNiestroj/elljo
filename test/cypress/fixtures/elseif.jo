{{#if visible}}
  <p>Hello world</p>
{{#elif visible2}}
  <p>Second world</p>
{{#else}}
  <p>Bye world</p>
{{/if}}

<button $click="toggleVisible">Toggle</button>

<script>
let visible = false;
let visible2 = false;

const toggleVisible = () => {
  if(!this.visible2) {
    this.visible2 = true
  } else {
    this.visible = !visible;
  }
}
</script>